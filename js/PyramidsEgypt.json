{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 800,
  "height": 500,
  "title": "Pyramids of Egypt",
  "projection": {"type": "mercator"},
  "layer": [
    {
      "data": {
        "url": "https://monashanthonynguyen.github.io/FIT3179/js/Egypt.json",
        "format": {"type": "topojson", "feature": "Egypt"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/MonashAnthonyNguyen/FIT3179/main/data/pyramids.csv"
      },
      "mark": {"type": "circle", "tooltip": {"content": "data"}},
      "encoding": {
        "longitude": {"field": "Longitude", "type": "quantitative"},
        "latitude": {"field": "Latitude", "type": "quantitative"}
      }
    }
  ]
}
